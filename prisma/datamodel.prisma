type Person {
  id: ID! @id
  kennitala: String!
  name: String!
  income: Float!
  address: String!
  personal_discount: Float!
  pension: Float!
  additional_pension: Float!
  can_calculate: Boolean!
  union_fees: Float!
  income_tax_rate: Float!
  spouseSsn: String
  expected_date_of_birth: DateTime!
}


type Submission {
  id: ID! @id
  timeframes: [TimeFrame]!
  person: Person!
  payPerMonth: Float
  payTotal:Float
  number_of_months: Int!

}

type TimeFrame {
  id: ID! @id
  start: DateTime!
  end: DateTime!
}